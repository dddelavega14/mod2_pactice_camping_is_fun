<%= form_for @sign_up do |f| %>
  <%= f.label :camper_id %>
  <%= f.collection_select :camper_id, Camper.all, :id, :name %>

  <%= f.label :activity_id %>
  <%= f.collection_select :activity_id, Activiy.all, :id, :name %>

  <%= f.label :time %>
  <%= f.number_field :time %>

  <%= f.submit  %>
<% end %>